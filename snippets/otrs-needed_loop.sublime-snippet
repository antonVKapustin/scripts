for my \$Needed ( qw($1) ) {
    # check needed stuff
    if ( !\$Param{ \$Needed } ) {
        \$Kernel::OM->Get('Kernel::System::Log')->Log(
            Priority => 'error',
            Message  => "Need \$Needed!",
        );
        return;
    }
}
